package br.com.aluno.aplicacao;

import java.util.Date;

import br.com.aluno.dao.AlunoDAO;
import br.com.aluno.model.Aluno;

public class Main {
	
	public static void main(String[] args) {
		
		AlunoDAO alunoDao = new AlunoDAO();
		
		Aluno aluno = new Aluno();
		
		aluno.setIdAluno(123);
		aluno.setCpfAluno(100260559);
		aluno.setNomeAluno("Filisbino");
		aluno.setNascAluno(new Date());
		aluno.setEmail("teste@teste.com");
		aluno.setSenha("senhateste");
		aluno.setTelefone("3463-7332");
		
		alunoDao.save(aluno);
	}
}
